package Interfaces;

import javax.swing.JFrame;

public class Ventana extends JFrame {
	public Ventana() {
		setSize(500,500);//tamaño
		setTitle("Primera Ventana");
		setLocation(450,200);//Para que cuando se ejecute abra lla ventana donde yo quiera
		
		setDefaultCloseOperation(EXIT_ON_CLOSE);// para que al dar click en la x se deje de ejecutar el programa
	}

}
